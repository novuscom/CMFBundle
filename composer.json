{
  "name": "novuscom/cmfbundle",
  "description": "Symfony2 based CMF",
  "type": "symfony-bundle",
  "homepage": "https://github.com/novuscom/CMFBundle",
  "license": "MIT",
  "keywords": [
	"cmf",
	"cmf bundle"
